{
  "name": "bittorrent-peerid",
  "description": "Map a BitTorrent peer ID to a human-readable client name and version",
  "version": "1.3.2",
  "author": {
    "name": "WebTorrent, LLC",
    "email": "feross@webtorrent.io",
    "url": "https://webtorrent.io"
  },
  "bugs": {
    "url": "https://github.com/webtorrent/bittorrent-peerid/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^12.12.7",
    "nodemon": "^1.19.4",
    "prettier": "1.19.0",
    "tape": "^4.6.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  },
  "keywords": [
    ".torrent",
    "bittorrent",
    "peer",
    "peer-to-peer",
    "torrent",
    "webtorrent"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/webtorrent/bittorrent-peerid.git"
  },
  "scripts": {
    "test": "npm run compile-utils && tape test/*.js",
    "compile-utils": "tsc ./src/lib/utils.ts",
    "start": "tsc && node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  }
}
